#include<stdlib.h>
#include<stddef.h>
#include<sys.h>
#include<stdio.h>
#include<string.h>


int main2()
{
		const char* x="root";
		const char* y="root";
		char* name = NULL;
		char* password = NULL;
		gets(name);
		gets(password);
		if(name == x)
		{	if(password == y)
			{
			printf("mm");
			}
			else
			printf("xx");
		}
		else
		{
			printf("zz");
		}
		int i = 0;
		while(name[i]!='0')
		{	printf(name[i]);
			printf(x[i]);
			i++;
		}

	return 0;
}
