#include<stdlib.h>
#include<stddef.h>
#include<sys.h>
#include<stdio.h>
#include<string.h>


int main2()
{
static const char* m = "root";
static const char* n = "yy";
char* name;
 char* password;
printf("UserId: ");
gets(name);
name[0] = 0;
if(name == m)
printf("true \n");
else printf("name Error; ");
printf("\n");
printf(name);
printf("\n");
printf("password: ");
gets(password);
password[0]=0;
printf("\n");
if(password == n)
printf("true \n");
else printf("pass Error \n;");
printf(password);
printf("\n");


		return 0;
}
